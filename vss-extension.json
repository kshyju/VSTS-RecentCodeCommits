{
  "manifestVersion": 1,
  "id": "RecentCodeCommitsExt",
  "version": "1.0.5",
  "name": "Recent Code Commits Widget",
  "description": "See most recent commits to your team project",
  "publisher": "kshyju",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "contributions": [
    {
      "id": "RecentCodeCommitsWidget",
      "type": "ms.vss-dashboards-web.widget",
      "targets": [
        "ms.vss-dashboards-web.widget-catalog"
      ],
      "properties": {
        "name": "Recent Code Commits",
        "description": "Simple widget to show the most recent code commits",
        "catalogIconUrl:": "img/logo.png",
        "previewImageUrl": "img/logo.png",
        "uri": "index.html",
        "supportedSizes": [
          {
            "rowSpan": 2,
            "columnSpan": 2
          }
        ],
        "supportedScopes": [ "project_team" ]
      }
    }
  ],
  "files": [
    {
      "path": "index.html",
      "addressable": true
    },
    {
      "path": "sdk/scripts",
      "addressable": true
    },
    {
      "path": "img",
      "addressable": true
    }
  ],
  "scopes": [
    "vso.work",
    "vso.code"
  ]
}